{
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "parser": "babel-eslint",
  "plugins": [
    "react",
    "import",
    "flowtype"
  ],
  "parserOptions": {
    "ecmaVersion": "6",
    "sourceType": "module"
  },
  "rules": {
    "array-bracket-spacing": [2, "never"],
    "arrow-body-style": 2,
    "arrow-parens": [2, "as-needed"],
    "arrow-spacing": 2,
    "block-spacing": [2, "always"],
    "brace-style": [2, "1tbs"],
    "comma-dangle": [2, "always-multiline"],
    "comma-spacing": [2, {"after": true, "before": false}],
    "comma-style": [2, "last"],
    "computed-property-spacing": [2, "never"],
    "curly": [2, "all"],
    "eol-last": 2,
    "eqeqeq": [2, "always", {"null": "ignore"}],
    "func-call-spacing": [2, "never"],
    "import/no-duplicates": 2,
    "indent": [2, 2, {"SwitchCase": 1}],
    "key-spacing": [2, {"afterColon": true, "beforeColon": false, "mode": "strict"}],
    "keyword-spacing": [2],
    "max-params": [1, {"max": 4}],
    "no-alert": 1,
    "no-dupe-args": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-empty-function": 1,
    "no-empty-pattern": 2,
    "no-eval": 1,
    "no-extend-native": 2,
    "no-extra-boolean-cast": 2,
    "no-fallthrough": 2,
    "no-floating-decimal": 2,
    "no-inner-declarations": 2,
    "no-loop-func": 2,
    "no-mixed-requires": 0,
    "no-nested-ternary": 0,
    "no-new-symbol": 2,
    "no-redeclare": 2,
    "no-restricted-globals": [2, "event", "location"],
    "no-sparse-arrays": 2,
    "no-tabs": 2,
    "no-this-before-super": 2,
    "no-trailing-spaces": 2,
    "no-underscore-dangle": [0],
    "no-unmodified-loop-condition": 1,
    "no-unreachable": 1,
    "no-unused-vars": [1, {"argsIgnorePattern": "^_", "varsIgnorePattern": "^_"}],
    "no-useless-constructor": 2,
    "no-var": 2,
    "no-with": 2,
    "object-curly-spacing": [2, "never"],
    "object-shorthand": 2,
    "operator-linebreak": [2, "after", {"overrides": {"?": "after", ":": "ignore"}}],
    "prefer-arrow-callback": 2,
    "prefer-const": 2,
    "prefer-spread": 2,
    "quotes": [2, "single", {"allowTemplateLiterals": true}],
    "require-yield": 2,
    "rest-spread-spacing": 2,
    "semi": [2, "always"],
    "space-before-blocks": 2,
    "space-before-function-paren": [2, {"anonymous": "always", "named": "never"}],
    "strict": [1, "never"],
    "template-curly-spacing": [2, "never"],
    "import/newline-after-import": [2, {"count": 2}],
    "react/jsx-closing-bracket-location": [2, "line-aligned"],
    "react/jsx-curly-spacing": [2, "never"],
    "react/jsx-indent": [2, 2],
    "react/jsx-indent-props": [2, 2],
    "react/jsx-no-undef": 2,
    "react/jsx-pascal-case": 2,
    "react/jsx-space-before-closing": 2,
    "react/jsx-uses-react": 2,
    "react/jsx-uses-vars": 2,
    "react/no-array-index-key": 1,
    "react/no-direct-mutation-state": 2,
    "react/no-string-refs": 1,
    "react/prop-types": 1,
    "react/react-in-jsx-scope": 2,
    "flowtype/semi": [2, "always"],
    "flowtype/delimiter-dangle": [2, "always-multiline"],
    "flowtype/space-after-type-colon": [2, "always"]
  }
}
